    "def three_d_split(l, r):\n",
    "    \"\"\"\n",
    "    Split to tuple of 3 lists : [0, l) [l,r) [r,n)\n",
    "    \"\"\"\n",
    "    a = {'X': X[0:l],  'Y': Y[0:l]}\n",
    "    b = {'X': X[l:r],  'Y': Y[l:r]}\n",
    "    c = {'X': X[r:],     'Y': Y[r:]}\n",
    "    return a, b, c\n",
    "\n",
    "def combine(a, c):\n",
    "    ax, cx = a['X'], c['X']\n",
    "    ay, cy = a['Y'], c['Y']\n",
    "    \n",
    "    combined = dict({'X': np.concatenate((ax, cx), axis=0), 'Y': np.concatenate((ay, cy), axis=0)})\n",
    "    return combined"
